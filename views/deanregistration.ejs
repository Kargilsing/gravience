<%- include('./header.ejs')  %>






<div class=" container-fluid  Register">
    
    <div class="container">
        <div class="row">
    
            

   <div class=" col-md-6 rg-us ">
    <h4 class="ml-11 text-center Your">Register To Your Account </h4>

    <% if(message.length > 0){ %>
      <div class="alert alert-danger">
        <%= message %>
       </div>
    <% } %>
                       
    <form class="ng-untouched ng-pristine ng-invalid  chh" action="/deanregister" method="post">
      <div class="form-group">
          <label>Username</label>
        <input type="text" name="name" placeholder="Name" class="form-control ng-untouched ng-pristine ng-invalid">
      </div>
      <br>
      <div class="form-group">
        <label>Email.Id</label>
        <input type="text" name="email" placeholder="email" class="form-control ng-untouched ng-pristine ng-invalid">
      </div>
      <br>
      <div class="form-group">
          <label>Password</label>
        <input type="password" name="C_password" placeholder="Password" class="form-control ng-untouched ng-pristine ng-invalid">
      </div>
      <br>
      <div class="form-group">
        <label>Confirm Password</label>
        <input type="password" name="confirm_password" placeholder="Confirm Password" class="form-control ng-untouched ng-pristine ng-invalid">
      </div>
      <br>
      <div class="form-group">
       
        <button class="rgbutton shadow-lg text-right " type="submit" value="Login" onclick="">Sign In</button>
        
         &nbsp; &nbsp;<span>Already have an account?</span><a href="/deanlogin"> Log In </a>
       
       
         
      </div>
    </form>
  </div>
  <div class="col-md-6"></div>		
</div>
</div>
</div>








<%- include('./footer.ejs')  %>