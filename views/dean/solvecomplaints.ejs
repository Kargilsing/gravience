<%- include('deanheader.ejs')%>
<%- include('deansidebar.ejs')%>


<div class="col-md-9 dash-left">
    <ul>
        <li><a href="#"><h4>Solve Complaints Display</h4></a></li>
        
    </ul>
    <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Sr.no</th>
                <th>Roll.No & Date:</th>
                <th>Complaint Type</th>
           
                <th>complaint Details</th>
               
                <th>complaint Replied</th>
               
              

            </tr>
        </thead>
        <tbody>
       
            <% i=1 %>
            <% view.forEach((item)=>{ %>
            <tr>
                <td><%= i++ %></td>
                <td><%= item.roll %>
                    <br><%= item.createdAt.toLocaleString() %>
                    
                </td> 
              <td><%= item.complaint_type %></td>  
              <td><%= item.com_details %></td>                       
              <td><%= item.com_reply %></td>
             
          
          </tr>
         
          <% }) %>
          
          

        </tbody>
    </table> 
    
            
    
</div>
</div>
</div>
</div>


<%- include('deanfooter.ejs')%>