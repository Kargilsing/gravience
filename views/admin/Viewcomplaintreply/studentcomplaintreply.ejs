<%- include('../adminsidebar.ejs')%>
<%- include('../adminheader.ejs')%>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header d-flex justify-content-between">
                        <div>
                          <strong class="card-title">Data Table</strong>
                        </div>
                       
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>Sr.no</th>
                                    <th>Roll.No & Date:</th>
                                    <th>Complaint Type</th>
                               
                                    <th>complaint Details</th>
                                   
                                    <th>complaint Replied</th>
                                   
                                    <th> Action</th>
  
                                </tr>
                            </thead>
                            <tbody>
                           
                                <% i=1 %>
                                <% view.forEach((item)=>{ %>
                                <tr>
                                    <td><%= i++ %></td>
                                    <td><%= item.roll %>
                                        <br><%= item.createdAt.toLocaleString() %>
                                        
                                    </td> 
                                  <td><%= item.complaint_type %></td>  
                                  <td><%= item.com_details %></td>                       
                                  <td><%= item.com_reply %></td>
                                 <td><a href="/replydelete/<%= item._id %>" class="btn btn-info"><i class="fa-solid fa-trash"></i></a>
                                    <a href="#" class="btn btn-info"><i class="fa-solid fa-eye-slash"></i></a>
                                
                                </td>
                              
                              </tr>
                             
                              <% }) %>
                              
                              
  
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- .animated -->
  
  
  </div>


<%- include('../adminfooter.ejs')%>