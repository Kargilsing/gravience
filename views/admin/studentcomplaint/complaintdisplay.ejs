<%- include('../adminsidebar.ejs')%>
<%- include('../adminheader.ejs')%>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header d-flex justify-content-between">
                        <div>
                          <strong class="card-title">Data Table</strong>
                        </div>
                       
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>Date</th>
                                    <th>Complaint Type</th>
                                    <th>Semester</th>
                                    <th>Roll.No:</th>
                                    <th>course_title</th>
                                   
                                    <th>Image</th>
                                    <th> Action</th>
  
                                </tr>
                            </thead>
                            <tbody>
                           
                           
                                <% com.forEach((items)=>{ %>
                                <tr>
                                    <td><%=  items.createdAt.toLocaleString() %></td>
                                  <td><%= items.complaint_type %></td>
                                  <td><%= items.semester %></td>
                                  <td><%= items.roll %></td> 
                                  <td><%= items.course_title %></td>
                                 

                                  <td><img src="<%= items.com_image.url %>" width="100px"></td>
                              <td>
                                  <a href="/complaintreply/<%= items._id %>" class="btn btn-info"><i class="fa-solid fa-pen-nib"></i> </a>
                                 
                                  
                              </td>
                              
                              </tr>
                              <% }) %>
                             
                              
                              
  
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- .animated -->
  
  
  </div>


<%- include('../adminfooter.ejs')%>